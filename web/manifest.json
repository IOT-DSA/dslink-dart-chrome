{
  "name": "DSLink",
  "version": "1",
  "minimum_chrome_version": "40",
  "manifest_version": 2,
  "background": {
    "scripts": ["link.dart.js"],
    "persistent": true
  },
  "permissions": [
    "alarms",
    "idle",
    "identity",
    "storage",
    "system.cpu",
    "system.memory",
    "tts",
    "topSites"
  ]
}
